<p id="notice"><%= notice %></p>

<h1>Listing Course User Tests</h1>

<table>
  <thead>
    <tr>
      <th>Course user</th>
      <th>Test</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_user_tests.each do |course_user_test| %>
      <tr>
        <td><%= course_user_test.course_user_id %></td>
        <td><%= course_user_test.test_id %></td>
        <td><%= link_to 'Show', course_user_test %></td>
        <td><%= link_to 'Edit', edit_course_user_test_path(course_user_test) %></td>
        <td><%= link_to 'Destroy', course_user_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course user test', new_course_user_test_path %>
