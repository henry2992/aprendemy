<p id="notice"><%= notice %></p>

<h1>Listing Student Course User Tests</h1>

<table>
  <thead>
    <tr>
      <th>Course user</th>
      <th>Test</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_course_user_tests.each do |student_course_user_test| %>
      <tr>
        <td><%= student_course_user_test.course_user_id %></td>
        <td><%= student_course_user_test.test_id %></td>
        <td><%= link_to 'Show', student_course_user_test %></td>
        <td><%= link_to 'Edit', edit_student_course_user_test_path(student_course_user_test) %></td>
        <td><%= link_to 'Destroy', student_course_user_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course user test', new_student_course_user_test_path %>
