}


/* Tablets (portrait and landscape) ----------- */
@media only screen and (min-width : 768px) {
    /*Landing NavBar*/

    .landing-links{
      display: inline-block;
    }

    /*Index Navbar*/
	   #logo{ display: block; }
	   #isotipo{ display: none; }
  	.nav-calendar-wrapper{ display: block !important;}
  	.nav-calendar{margin-top: 0px; }
  	.nav-profile-wrapper{ display: inline-block !important; }
  	.mobile-menu-icon{ display: none !important; }
    /* Courses NavBar */
    #logo-courses{ display: block; }
    #isotipo-courses{ display: none; }
    /* Vertical NavBar */
    #vertical-nav { display: block; }
    /* Course Content */
    #container { padding-left: 50px; margin-top: 55px; }


    /************ Landing Init **********/

    .landing-init {
      min-height: 100vh; 
    }

    .landing-title {
      font-size: 48px;
    }

    .landing-wrapper{
      max-width: 650px;
    }


    .landing-wrapper p{
      font-size: 18px;
    }

    .logo-landing{
      margin: 60px auto 25px auto;
    }

    /************ Know More **************/
    .know-more-wrapper{
      padding: 150px 0;
    }

    .know-more-content p {
      font-size: 18px;
    }

    /************* How works *********/

    .how-works{
        padding: 50px 0;
    }

    .how-works-title p {
